{
  "expo": {
    "name": "rentTracker",
    "slug": "rentTracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/home_icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.zahir111.rentTracker",
      "googleServicesFile": "./JsonFiles/google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}